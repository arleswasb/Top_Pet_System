[flake8]
max-line-length = 127
max-complexity = 10
exclude = 
    migrations/
    __pycache__/
    manage.py
    settings/
    venv/
    env/
    .git/
    .tox/
    build/
    dist/
ignore = 
    E501,  # line too long
    W503,  # line break before binary operator
    E203,  # whitespace before ':'
    F401,  # imported but unused (in __init__.py files)
per-file-ignores =
    __init__.py:F401
    */migrations/*:E501,F401
    */tests/*:E501
    */test_*:E501
