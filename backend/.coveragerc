[run]
source = .
omit = 
    */migrations/*
    */venv/*
    */env/*
    */tests/*
    */test_*
    manage.py
    */settings/*
    */__pycache__/*
    */htmlcov/*
    */.git/*
    *.pyc
    */logs/*
    */media/*
    */static/*
    conftest.py
    setup.py
    */wsgi.py
    */asgi.py
    */__pycache__/*
    */node_modules/*
    .tox/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
